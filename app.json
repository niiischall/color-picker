{
  "expo": {
    "name": "Color-Picker",
    "slug": "Color-Picker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/new-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.colorPicker.colorPickerApp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Find out how good you are with RGB colour values with this fun, little mobile game built with React Native. ",
    "githubUrl": "https://github.com/nischalNikit/color-picker"
  }
}
